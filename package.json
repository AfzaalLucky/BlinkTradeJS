{
  "name": "blinktrade",
  "version": "0.0.1",
  "description": "BlinkTrade Client for node.js",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "flow": "flow check",
    "check": "npm run lint && npm run test ",
    "build": "webpack ./index.js --config webpack.dev.js dist/bundle.js",
    "build:example:node": "webpack examples/node.js --config webpack.dev.js examples/bundle/bundle-node.js",
    "build:example:rest": "webpack examples/rest.js --config webpack.dev.js examples/bundle/bundle-rest.js",
    "build:examples": "npm run build:example:node && npm run build:example:rest",
    "build:examples:blinktrade": "cd examples && rm -rf node_modules && npm install && cd .. && npm run build:examples",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive"
  },
  "keywords": [
    "blinktrade",
    "node"
  ],
  "author": "Cesar August, Rodrigo Souza",
  "license": "GPLv3",
  "dependencies": {
    "fetch-jsonp": "^1.0.1",
    "fingerprintjs2": "^1.4.1",
    "getmac": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "node-fetch": "^1.6.0",
    "node-uuid": "^1.4.7",
    "sjcl": "^1.0.3",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint_d": "^4.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "moment": "^2.14.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "webpack": "^1.13.2"
  }
}
